{
	"compileOnSave": false,
	"compilerOptions": {
		"outDir": "./dist/out-tsc",
		"strict": true,
		"noImplicitOverride": true,
		"noPropertyAccessFromIndexSignature": true,
		"noImplicitReturns": true,
		"noFallthroughCasesInSwitch": true,
		"skipLibCheck": true,
		"isolatedModules": false,
		"esModuleInterop": true,
		"sourceMap": true,
		"declaration": false,
		"experimentalDecorators": true,
		"moduleResolution": "bundler",
		"importHelpers": true,
		"target": "ES5",
		"module": "ESNext",
		"allowJs": true,
		"lib": ["ES2023"],
		"types": ["node", "express", "multer", "cors", "redis"],
		"baseUrl": ".",
		"paths": {
			"@/*": ["src/*"],
			"@common/*": ["src/common/*"],
			"@modules/*": ["src/modules/*"],
			"@di": ["src/di/index.ts"]
		}
	}
}
